<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0"
            prefWidth="600.0"
            style="-fx-background-color: #2f3447;"
            xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.course.SignUpController">
    <children>
        <TextField fx:id="username" layoutX="204.0" layoutY="96.0" maxWidth="600.0" prefHeight="25.0" prefWidth="189.0"
                   promptText="UserName" style="-fx-background-color: #2f3447; -fx-border-color: #fff;"
                   AnchorPane.leftAnchor="204.0" AnchorPane.rightAnchor="206.66666666666663">
            <font>
                <Font size="14.0"/>
            </font>
        </TextField>
        <PasswordField fx:id="password" layoutX="205.0" layoutY="152.0" prefHeight="25.0" prefWidth="189.0"
                       promptText="Password" style="-fx-background-color: #2f3447; -fx-border-color: #fff;">
            <font>
                <Font size="14.0"/>
            </font>
        </PasswordField>
        <Button fx:id="signup" layoutX="234.0" layoutY="298.0" mnemonicParsing="false" onAction="#signupAction"
                prefHeight="29.0" prefWidth="129.0" style="-fx-background-color: #3162a4;" text="SignUp "
                textFill="WHITE"/>
        <ImageView fitHeight="67.0" fitWidth="65.0" layoutX="267.0" layoutY="14.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@Images/logo.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="41.0" fitWidth="29.0" layoutX="168.0" layoutY="98.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@Images/icons8_User_Male_26px.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="41.0" fitWidth="29.0" layoutX="168.0" layoutY="154.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@Images/icons8_Lock_2_64px.png"/>
            </image>
        </ImageView>
        <PasswordField fx:id="location" layoutX="205.0" layoutY="247.0" prefHeight="25.0" prefWidth="189.0"
                       promptText="Your Location" style="-fx-background-color: #2f3447; -fx-border-color: #fff;">
            <font>
                <Font size="14.0"/>
            </font>
        </PasswordField>
        <ImageView fitHeight="29.0" fitWidth="29.0" layoutX="168.0" layoutY="198.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@Images/icons8_Genderqueer_26px.png"/>
            </image>
        </ImageView>
        <RadioButton fx:id="male" layoutX="210.0" layoutY="204.0" mnemonicParsing="false" text="Male" textFill="WHITE"/>
        <RadioButton fx:id="female" layoutX="299.0" layoutY="204.0" mnemonicParsing="false" text="Female"
                     textFill="WHITE"/>
        <ImageView fitHeight="32.0" fitWidth="29.0" layoutX="168.0" layoutY="249.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@Images/icons8_Marker_26px.png"/>
            </image>
        </ImageView>
        <ImageView fx:id="process" fitHeight="53.0" fitWidth="48.0" layoutX="370.0" layoutY="295.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@Images/output-onlinegiftools.gif"/>
            </image>
        </ImageView>
        <Button fx:id="backLogIn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#loginAction"
                prefHeight="36.0" prefWidth="41.0" style="-fx-background-color: #3162a4;" text="&lt;&lt;"
                textFill="WHITE"/>
    </children>
</AnchorPane>
